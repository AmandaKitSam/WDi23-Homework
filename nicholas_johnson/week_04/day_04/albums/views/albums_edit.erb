<h2>Edit an existing submission</h2>

<form action="/albums" method="post">
	<input type="text" name="title" value="<%=@albums['title']%>">
	<input type="text" name="artist" value="<%=@albums['artist']%>">
	<input type="text" name="year" value="<%=@albums['year']%>">
	<input type="text" name="label" value="<%=@albums['label']%>">
	<input type="url" name="image" value="<%=@albums['image']%>">
	<button>Update</button>
</form>
